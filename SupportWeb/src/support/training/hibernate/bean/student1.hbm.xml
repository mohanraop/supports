<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="support.training.hibernate.bean">


	<class name="Student1" table="student1">
		<id name="sid" column="id">
			<generator class="native" />
		</id>
		<property name="st_name" column="st_name"></property>
		<property name="st_class" column="st_class"></property>
		<property name="st_sub1" column="st_sub1"></property>
	</class>
    

</hibernate-mapping>